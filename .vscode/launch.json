{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Electron: Main Process",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/.bin/electron-forge",
        "args": ["start"],
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        // "protocol": "inspector",
        "env": {
          "NODE_ENV": "development"
        }
      },
      {
        "name": "Electron: Renderer Process",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true
      }
    ]
  }
  